        msg="â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬\n"
        msg+="âœŒï¸ğŸ˜ƒ Ola <b>${message_from_first_name[$id]}</b>, SEJA BEM VINDO!(a)\n"
        msg+="â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬\n"
        msg+="<b>GERE SEU TESTE DE 2 HORAS GRÃTIS</b>\n"
        msg+="â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬â–¬\n"
        msg+="<b>ğŸ”´CLARO ğŸŸ£VIVO ğŸ”µTIM</b>\n"
        ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
        --text "$(echo -e $msg)" \
        --reply_markup "$keyboard1" \
        --parse_mode html
        return 0
}

